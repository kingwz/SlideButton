<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlideButton">
        <attr name="cover_drawable" format="reference|color"/>
        <attr name="bg_color" format="color"/>
        <attr name="inner_text_color" format="color"/>
        <attr name="cover_text_color" format="color"/>
        <attr name="inner_text_size" format="dimension"/>
